<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox padding="$x1" prefHeight="288.0" prefWidth="771.0" xmlns:fx="http://javafx.com/fxml" fx:controller="dam.gestorclub.controladores.externos.ExternosController">
  <children>
    <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
        <TableView fx:id="tvExternos" editable="true" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
          <columns>
            <TableColumn prefWidth="50.0" text="ID" fx:id="tcEId" />
            <TableColumn prefWidth="200.0" text="Nombre" fx:id="tcENombre" />
            <TableColumn prefWidth="200.0" text="Apellidos" fx:id="tcEApellidos" />
            <TableColumn prefWidth="200.0" text="Empleo" fx:id="tcEEmpleo" />
            <TableColumn prefWidth="75.0" text="Empresa" fx:id="tcEEmpresa" />
          </columns>
        </TableView>
        <Button fx:id="bEEliminar" mnemonicParsing="false" onAction="#onEliminarClicked" text="Eliminar">
          <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
    <FlowPane>
      <children>
        <TextField fx:id="tfENombre" prefWidth="179.0" promptText="Nombre">
          <FlowPane.margin>
            <Insets left="50.0" />
          </FlowPane.margin>
        </TextField>
        <TextField id="tfALugar" fx:id="tfEApellidos" prefWidth="120.0" promptText="Apellidos" />
        <TextField id="tfACategoria" fx:id="tfEEmpleo" prefWidth="138.0" promptText="Empleo" />
        <TextField fx:id="tfEEmpresa" prefWidth="142.0" promptText="Empresa" />
        <Button mnemonicParsing="false" onAction="#onAddClicked" text="AÃ±adir">
          <FlowPane.margin>
            <Insets left="10.0" />
          </FlowPane.margin>
        </Button>
      </children>
      <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
      </VBox.margin>
    </FlowPane>
    <ToolBar>
      <items>
        <Button mnemonicParsing="false" onAction="#onVolverClicked" text="Volver" />
      </items>
    </ToolBar>
  </children>
</VBox>
