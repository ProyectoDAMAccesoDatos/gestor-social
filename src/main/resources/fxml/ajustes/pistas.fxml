<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox padding="$x1" prefHeight="288.0" prefWidth="771.0" xmlns:fx="http://javafx.com/fxml" fx:controller="dam.gestorclub.controladores.ajustes.PistasController">
  <children>
    <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
        <TableView fx:id="tvPistas" editable="true" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
          <columns>
            <TableColumn fx:id="tcPId" editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="103.0" text="ID" />
            <TableColumn fx:id="tcPNombre" prefWidth="250.0" text="Nombre" />
            <TableColumn fx:id="tcPPrecioSocios" prefWidth="120.0" text="Precio socios" />
            <TableColumn fx:id="tcPPrecioNoSocios" maxWidth="5000.0" minWidth="10.0" prefWidth="124.0" text="Precio no socios" />
          </columns>
        </TableView>
        <Button mnemonicParsing="false" fx:id="bPEliminar" onAction="#onEliminarClicked" text="Eliminar">
          <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
    <FlowPane prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <TextField fx:id="tfPNombre" prefWidth="250.0" promptText="Nombre">
          <FlowPane.margin>
            <Insets left="103.0" />
          </FlowPane.margin>
        </TextField>
        <TextField id="tfPNoSocios" fx:id="tfPPrecioSocios" prefWidth="120.0" promptText="Precio socios" />
        <TextField id="tfPrecioNoSocios" fx:id="tfPPrecioNoSocios" prefWidth="124.0" promptText="Precio no socios" />
        <Button mnemonicParsing="false" onAction="#onAddClicked" text="AÃ±adir">
          <FlowPane.margin>
            <Insets left="10.0" />
          </FlowPane.margin>
        </Button>
      </children>
      <VBox.margin>
        <Insets bottom="5.0" top="5.0" />
      </VBox.margin>
    </FlowPane>
  </children>
</VBox>
